#define PIN_BRIGHT 9 
#define Y_POSITION 8  // vertical position, center screen value: 4


// graphics library
#include <GyverOLED.h>
GyverOLED<SSD1306_128x64, OLED_NO_BUFFER> oled; // if you have not default I2C address of the screen - pass it here: oled(address);

void InitDisp() {
  pinMode(PIN_BRIGHT, INPUT_PULLUP);

  oled.init();
  oled.flipV(true);  // flip screen 180deg
  oled.flipH(true);  //
  oled.clear();
}

void PrintDebug() {
  if (digitalRead(PIN_BRIGHT)) oled.setContrast(0);
  else                         oled.setContrast(255);

  oled.textMode(BUF_REPLACE);

#define SCALE_MENU 2
  oled.setScale(SCALE_MENU);
  for (int i = 0; i < MENU_SIZE; i++) {
    oled.setCursor(0, i * SCALE_MENU);
    oled.invertText(i == cursor);
    oled.print(menu[i].sym);
    oled.print(*menu[i].data);
  }
  oled.invertText(false);

  float unit = Unit();  
  unsigned int speed = GetSpeedInterval(500) / unit;

  oled.setCursorXY(74, 0);
  oled.print(acc100calc);
  oled.print("  ");

#define SCALE_SPEED 3
  oled.setScale(SCALE_SPEED);
  oled.setCursorXY(74, 20);
  oled.print(speed);
  oled.print("  ");
}



// convert speed to miles if requred and
// print images of digits to the screen
void PrintSpeed() {
  // static unsigned long prev = 0;
  // unsigned long time = millis();
  // if (time - prev < 100) return;
  // prev = time;

  float unit = Unit();  

  if (digitalRead(PIN_BRIGHT)) oled.setContrast(0);
  else                         oled.setContrast(255);
  
  unsigned int speed = GetSpeedInterval(500) / unit;
  speed = speed % 999;
  unsigned int d1, d2, d3;
  d1 = speed / 100;		          // 100
  d2 = (speed % 100) / 10;    	//  10
  d3 = (speed % 10);		        //   1

  if (d1) {
    oled.drawBitmap(4, Y_POSITION, frames[d1], 40, 56);
  } else {
// debug print    oled.clear(4, Y_POSITION + 8, 43, Y_POSITION + 39);
    if (unit <= 1) oled.drawBitmap(4, Y_POSITION + 40, frame_kmh_40x16, 40, 16); // kilometers
    else           oled.drawBitmap(4, Y_POSITION + 40, frame_mh_40x16, 40, 16);  // miles
  }
  
  if (d1 || d2) oled.drawBitmap(44, Y_POSITION, frames[d2], 40, 56);
  else          oled.clear(44, Y_POSITION, 83, Y_POSITION + 55);
                oled.drawBitmap(84, Y_POSITION, frames[d3], 40, 56);

// debug print
  oled.setScale(2);
  oled.home();
  oled.textMode(BUF_REPLACE);
  oled.print(acc100);
  oled.print("  ");
  oled.setCursor(0, 2);
  oled.print(acc100calc);
  oled.print("  ");
}
